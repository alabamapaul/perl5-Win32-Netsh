<?xml version="1.0"?>
<!-- #################################################################### -->
<!-- ##        File: win32-netsh-wlan-single.xml                          -->
<!-- ## Description: Microsoft Wireless Profile for use when testing the  -->
<!-- ##              Win32::Netsh::Wlan module                            -->
<!-- ##              This profile only has one SSID                       -->
<!-- #################################################################### -->
<WLANProfile xmlns="http://www.microsoft.com/networking/WLAN/profile/v1">
	<name>Win32-Netsh-Wlan-Single</name>
	<SSIDConfig>
		<SSID>
			<name>SSID-Single</name>
		</SSID>
		<nonBroadcast>false</nonBroadcast>
	</SSIDConfig>
	<connectionType>ESS</connectionType>
	<connectionMode>auto</connectionMode>
	<autoSwitch>false</autoSwitch>
	<MSM>
		<security>
			<authEncryption>
				<authentication>open</authentication>
				<encryption>none</encryption>
				<useOneX>false</useOneX>
			</authEncryption>
		</security>
	</MSM>
</WLANProfile>
